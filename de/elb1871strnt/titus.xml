        <verse osisID='Titus.3.10'>Einen <w lemma='strong:H141'>sektiererischen </w><w lemma='strong:H444'>Menschen </w>weise <w lemma='strong:H3868'>ab </w><w lemma='strong:H3326'>nach </w>einer <w lemma='strong:H3391'>ein- </w><w lemma='strong:H2532'>und </w><w lemma='strong:H1208'>zweimaligen </w><w lemma='strong:H3559'>Zurechtweisung</w>,</verse>
