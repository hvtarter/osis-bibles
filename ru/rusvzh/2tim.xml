        <verse osisID='2Tim.4.8'>а <w lemma='strong:H3063'>теперь </w><w lemma='strong:H606'>готовится </w><w lemma='strong:H3427 strong:H3588 strong:H3588'>мне </w><w lemma='strong:H4735'>венец </w><w lemma='strong:H1343'>правды </w>, <w lemma='strong:H3739'>который </w><w lemma='strong:H591'>даст </w><w lemma='strong:H3427 strong:H3588'>мне </w><w lemma='strong:H2962'>Господь </w><w lemma='strong:H3588'>, </w><w lemma='strong:H1342'>праведный </w><w lemma='strong:H2923'>Судия </w>, <w lemma='strong:H1722 strong:H3588'>в </w><w lemma='strong:H2250'>день </w><w lemma='strong:H1565'>оный </w>; <w lemma='strong:H1161'>и </w><w lemma='strong:H3756'>не </w><w lemma='strong:H3440'>только </w><w lemma='strong:H1698'>мне </w>, <w lemma='strong:H235'>но </w><w lemma='strong:H2532'>и </w><w lemma='strong:H3956'>всем </w><w lemma='strong:H3588'>, </w><w lemma='strong:H25 strong:H3588'>возлюбившим </w><w lemma='strong:H2015'>явление </w><w lemma='strong:H846'>Его </w>. </verse>
        <verse osisID='2Tim.4.9'>
          <w lemma='strong:H4704'>Постарайся </w>
          <w lemma='strong:H2064'>придти </w>
